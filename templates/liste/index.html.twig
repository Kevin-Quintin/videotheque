{% extends 'base.html.twig' %}

{% block title %}Video Dev{% endblock %}

{% block body %}

<div class="container">
<div class="row">
{% for movie in movies %}

<div class="col-sm-3 my-3 g-4">
    <a href="details/{{ movie.id }}">
        <img src="assets/affiches-film/{{ movie.image }}" alt="affiche de film" class="h-100 w-100">
    </a>
</div>

{% endfor %}
</div>
</div>
{% endblock %}
